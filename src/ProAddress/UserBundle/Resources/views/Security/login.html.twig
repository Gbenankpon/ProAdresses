    <body class="login-layout light-login ">
        <div class="main-container">
	        <div class="main-content">
		        <div class="row">
			        <div class="col-xs-10 col-sm-10 col-md-10 col-sm-offset-1">
				        <div class="login-container">
					        <div class="center">
						        <h1><a href="{{ path('pa_app_accueil') }}" class="text-muted"><b>Pro</b>Address</a></h1>
					        </div>
					        <div class="space-6"></div>
					        <div class="position-relative">
						        <div id="login-box" class="login-box visible widget-box no-border">
							        <div class="widget-body">
								        <div class="widget-main">
									        <h4 class="header blue lighter bigger center">Merci de vous connecter</h4>

									        <div class="space-6"></div>
                                            {% if error %}
												<div>{{ error.messageKey|trans(error.messageData, 'security') }}</div>
                                            {% endif %}
                                            {% if error  %}
										        <div class="alert alert-danger">{% if error.messageKey is defined and error.messageKey is not empty %}{{ error.messageKey|trans(error.messageData, 'security') }}{% endif %}</div>
										        <div class="space-6"></div>
                                            {% endif %}
                            <!-- form begin -->
	                                        <form action="{{ path('fos_user_security_check') }}" method="post">
                                            {% if csrf_token %}
									        <input type="hidden" name="_csrf_token" value="{{ csrf_token }}" />
                                            {% endif %}
									        <fieldset>
									            <label class="block clearfix">
										            <span class="block input-icon input-icon-right">
										                <input type="text" id="username" name="_username" value="{{ last_username }}" required="required" class="form-control" placeholder="{{ 'security.login.username'|trans({}, 'FOSUserBundle') }}" />
											            {#<i class="glyphicon glyphicon-user form-control-feedback"></i>#}
										            </span>
										        </label>
										        <label class="block clearfix">
											        <span class="block input-icon input-icon-right">
												        <input type="password" id="password" name="_password" required="required" class="form-control" placeholder="{{ 'security.login.password'|trans({}, 'FOSUserBundle') }}" />
												        {#<i class="glyphicon glyphicon-lock form-control-feedback"></i>#}
											        </span>
										        </label>
										        <input type="checkbox" id="remember_me" name="_remember_me" value="on" />
            <label for="remember_me">{{ 'security.login.remember_me'|trans({}, 'FOSUserBundle') }}</label>
										        <div class="space"></div>
										        <div class="clearfix">
											        <button type="submit" id="_submit" name="_submit" class="width-100 pull-right btn btn-sm btn-success">
												        <span class="bigger-110">{{ 'security.login.submit'|trans({}, 'FOSUserBundle') }}</span>
											        </button>
										        </div>
                                                <div class="space-4"></div>
									        </fieldset>
								        </form>
							            <div class="space-6"></div>
							        </div><!-- /.widget-main -->
							        <div class="toolbar clearfix">
								        <div>
									        <a href="{{ path('fos_user_resetting_request') }}" class="forgot-password-link" style="color: white">{{ 'Mot de passe oubli√©'|trans({}, 'FOSUserBundle') }}</a>
								        </div>
								        <div>
									        <a href="{{ path('fos_user_registration_register') }}" class="user-signup-link" style="color: white">{{ 'S\'enregistrer'|trans({}, 'FOSUserBundle') }}</a>
								        </div>
							        </div>
						        </div><!-- /.widget-body -->
					        </div><!-- /.login-box -->
					        </div><!-- /.position-relative -->
				        </div>
			        </div><!-- /.col -->
		        </div><!-- /.row -->
	        </div><!-- /.main-content -->
        </div><!-- /.main-container -->
